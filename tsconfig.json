{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ES2018",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "lib": [
            "es2020",
        ],
        "inlineSourceMap": true,
        "newLine": "CRLF"
    },
    "exclude": [
        "node_modules"
    ],
    "include": [
        "**.ts"
    ]
}